{
  "author": "YOU <YOU@example.com>",
  "name": "journal-backend",
  "description": "Quick Journal - Actionhero.js backend with GraphQL and MongoDB",
  "version": "0.1.0",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@apollo/server": "^5.1.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "actionhero": "29.3.4",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "graphql": "^16.12.0",
    "graphql-tag": "^2.12.6",
    "graphql-tools": "^9.0.21",
    "ioredis": "latest",
    "ioredis-mock": "latest",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.20.0",
    "winston": "latest",
    "ws": "latest"
  },
  "devDependencies": {
    "@types/formidable": "^3.4.6",
    "@types/glob": "latest",
    "@types/jest": "latest",
    "@types/node": "latest",
    "jest": "latest",
    "prettier": "latest",
    "ts-jest": "latest",
    "ts-node-dev": "latest",
    "type-fest": "latest",
    "typescript": "latest"
  },
  "scripts": {
    "dev": "ts-node-dev --no-deps --transpile-only ./src/server",
    "debug": "tsc && ts-node-dev --transpile-only --no-deps --inspect -- ./src/server ",
    "start": "node ./dist/server.js",
    "actionhero": "actionhero",
    "test": "jest",
    "pretest": "npm run build && npm run lint",
    "build": "tsc",
    "lint": "prettier --check src/*/** __tests__/*/**",
    "pretty": "prettier --write src/*/** __tests__/*/**"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    }
  }
}
